#include<iostream>
#include<numeric>
#include<vector>
#include<algorithm>
using namespace std;

void solve(){
    int n;
    cin>>n;
    if (n==3){
        cout<<"3\n";
    }
    else{
        cout<<"2\n";
    }
}

int main(){

    int t;
    cin>>t;
    while(t--){
        solve();
    }

    return 0;
}
