//Question URL: https://codeforces.com/contest/1977/problem/A
//Rating: 800

#include<iostream>
using namespace std;

void solve(){
    int n,m;
    cin>>n>>m;
    if (n<m){
        cout<<"NO\n";
        return;
    }
    if ((n-m)%2){
        cout<<"NO\n";
        return;
    }
    cout<<"YES\n";
}

int main(){

    int t;
    cin>>t;
    while(t--){
        solve();
    }

    return 0;
}
