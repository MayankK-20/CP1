//Question URL: https://codeforces.com/group/8bhJZQiaub/contest/529177/problem/D

#include<vector>
#include<algorithm>
using namespace std;
 
int main(){
    int n, m;
    cin>>n>>m;
    int ans=0;
    vector<int> v(n);
    for (int i=0; i<n; i++){
        cin>>v[i];
        if (m%v[i]==0){
            ans++;
        }
    }
    cout<<ans<<endl;
 
    return 0;
}
