//Question URL: https://codeforces.com/contest/1956/problem/A
//Rating: 800

#include<iostream>
#include<numeric>
#include<vector>
#include<algorithm>
using namespace std;

void solve(){
    int k,q;
    cin>>k>>q;
    int first;
    cin>>first;
    for (int i=1; i<k; i++){
        int num;
        cin>>num;
    }
    for (int i=0; i<q; i++){
        int num;
        cin>>num;
        if (first<=num){
            cout<<first-1<<" ";
        }
        else{
            cout<<num<<" ";
        }
    }
    cout<<endl;
}

int main(){

    int t;
    cin>>t;
    while(t--){
        solve();
    }

    return 0;
}
