#include<iostream>
#include<numeric>
#include<vector>
#include<algorithm>
using namespace std;

void solve(){
    int n;
    cin>>n;
    cout<<n/12<<endl;
}

int main(){

    int t;
    cin>>t;
    while(t--){
        solve();
    }

    return 0;
}
